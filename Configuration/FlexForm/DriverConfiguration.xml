<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<T3DataStructure xml:space="preserve">
    <sheets>
        <general>
            <ROOT>
                <TCEforms>
                    <sheetTitle>LLL:EXT:falsftp/Resources/Private/Language/locallang.xlf:flexform.general</sheetTitle>
                </TCEforms>
                <el>
                    <hostname>
                        <TCEforms>
                            <label>LLL:EXT:falsftp/Resources/Private/Language/locallang.xlf:flexform.general.hostname</label>
                            <config>
                                <type>input</type>
                                <size>33</size>
                                <eval>trim,required</eval>
                            </config>
                        </TCEforms>
                    </hostname>
                    <port>
                        <TCEforms>
                            <label>LLL:EXT:falsftp/Resources/Private/Language/locallang.xlf:flexform.general.port</label>
                            <config>
                                <type>input</type>
                                <size>33</size>
                                <eval>trim,required</eval>
                            </config>
                        </TCEforms>
                    </port>
                    <username>
                        <TCEforms>
                            <label>LLL:EXT:falsftp/Resources/Private/Language/locallang.xlf:flexform.general.username</label>
                            <config>
                                <type>input</type>
                                <size>33</size>
                                <eval>trim,required</eval>
                            </config>
                        </TCEforms>
                    </username>
                    <rootLevel>
                        <TCEforms>
                            <label>LLL:EXT:falsftp/Resources/Private/Language/locallang.xlf:flexform.general.rootLevel</label>
                            <config>
                                <type>input</type>
                                <size>33</size>
                                <eval>trim,required</eval>
                            </config>
                        </TCEforms>
                    </rootLevel>
                    <publicUrl>
                        <TCEforms>
                            <label>LLL:EXT:falsftp/Resources/Private/Language/locallang.xlf:flexform.general.publicUrl</label>
                            <config>
                                <type>input</type>
                                <size>33</size>
                                <eval>trim</eval>
                            </config>
                        </TCEforms>
                    </publicUrl>
                    <authenticationMethod>
                        <TCEforms>
                            <label>LLL:EXT:falsftp/Resources/Private/Language/locallang.xlf:flexform.general.authenticationMethod</label>
                            <onChange>reload</onChange>
                            <config>
                                <type>select</type>
                                <items>
                                    <numIndex index="0">
                                        <numIndex index="0">Please choose ...</numIndex>
                                        <numIndex index="1"/>
                                    </numIndex>
                                    <numIndex index="1">
                                        <numIndex index="0">Password</numIndex>
                                        <numIndex index="1">1</numIndex>
                                    </numIndex>
                                    <numIndex index="2">
                                        <numIndex index="0">Public Key</numIndex>
                                        <numIndex index="1">2</numIndex>
                                    </numIndex>
                                </items>
                                <eval>required</eval>
                            </config>
                        </TCEforms>
                    </authenticationMethod>
                    <adapter>
                        <TCEforms>
                            <label>LLL:EXT:falsftp/Resources/Private/Language/locallang.xlf:flexform.general.adapter</label>
                            <onChange>reload</onChange>
                            <config>
                                <type>select</type>
                                <itemsProcFunc>VerteXVaaR\FalSftp\Environment\Detector->getItemsForAdapterSelection</itemsProcFunc>
                                <items>
                                    <numIndex index="0">
                                        <numIndex index="0">Please choose ...</numIndex>
                                        <numIndex index="1"/>
                                    </numIndex>
                                </items>
                                <eval>required</eval>
                            </config>
                        </TCEforms>
                    </adapter>
                    <experts>
                        <TCEforms>
                            <label>LLL:EXT:falsftp/Resources/Private/Language/locallang.xlf:flexform.general.experts</label>
                            <onChange>reload</onChange>
                            <config>
                                <type>check</type>
                                <items>
                                    <numIndex index="0">
                                        <numIndex index="0">LLL:EXT:falsftp/Resources/Private/Language/locallang.xlf:flexform.general.experts.enable</numIndex>
                                        <numIndex index="1">1</numIndex>
                                    </numIndex>
                                </items>
                            </config>
                        </TCEforms>
                    </experts>
                </el>
            </ROOT>
        </general>
        <password>
            <ROOT>
                <TCEforms>
                    <sheetTitle>LLL:EXT:falsftp/Resources/Private/Language/locallang.xlf:flexform.password</sheetTitle>
                    <displayCond>FIELD:general.authenticationMethod:=:1</displayCond>
                </TCEforms>
                <el>
                    <password>
                        <TCEforms>
                            <label>LLL:EXT:falsftp/Resources/Private/Language/locallang.xlf:flexform.password.password</label>
                        </TCEforms>
                        <config>
                            <type>input</type>
                            <size>33</size>
                            <eval>password,required</eval>
                        </config>
                    </password>
                </el>
            </ROOT>
        </password>
        <pubkey>
            <ROOT>
                <TCEforms>
                    <sheetTitle>LLL:EXT:falsftp/Resources/Private/Language/locallang.xlf:flexform.pubkey</sheetTitle>
                    <displayCond>FIELD:general.authenticationMethod:=:2</displayCond>
                </TCEforms>
                <el>
                    <publicKey>
                        <TCEforms>
                            <label>LLL:EXT:falsftp/Resources/Private/Language/locallang.xlf:flexform.pubkey.publicKey</label>
                            <config>
                                <type>input</type>
                                <size>33</size>
                                <eval>input</eval>
                            </config>
                        </TCEforms>
                    </publicKey>
                    <privateKey>
                        <TCEforms>
                            <label>LLL:EXT:falsftp/Resources/Private/Language/locallang.xlf:flexform.pubkey.privateKey</label>
                            <config>
                                <type>input</type>
                                <size>33</size>
                                <eval>input</eval>
                            </config>
                        </TCEforms>
                    </privateKey>
                    <privateKeyPassword>
                        <TCEforms>
                            <label>LLL:EXT:falsftp/Resources/Private/Language/locallang.xlf:flexform.pubkey.privateKeyPassword</label>
                            <config>
                                <type>input</type>
                                <size>33</size>
                                <eval>password</eval>
                            </config>
                        </TCEforms>
                    </privateKeyPassword>
                </el>
            </ROOT>
        </pubkey>
        <experts>
            <ROOT>
                <TCEforms>
                    <sheetTitle>LLL:EXT:falsftp/Resources/Private/Language/locallang.xlf:flexform.experts</sheetTitle>
                    <displayCond>FIELD:general.experts:=:1</displayCond>
                </TCEforms>
                <el>
                    <foreignKeyFingerprint>
                        <TCEforms>
                            <label>LLL:EXT:falsftp/Resources/Private/Language/locallang.xlf:flexform.experts.foreignKeyFingerprint</label>
                            <config>
                                <type>input</type>
                                <size>33</size>
                                <eval>trim</eval>
                            </config>
                        </TCEforms>
                    </foreignKeyFingerprint>
                    <foreignKeyFingerprintMethod>
                        <TCEforms>
                            <label>LLL:EXT:falsftp/Resources/Private/Language/locallang.xlf:flexform.experts.foreignKeyFingerprintMethod</label>
                            <config>
                                <type>select</type>
                                <items>
                                    <numIndex index="0">
                                        <numIndex index="0">LLL:EXT:falsftp/Resources/Private/Language/locallang.xlf:flexform.experts.foreignKeyFingerprintMethod.sha1</numIndex>
                                        <numIndex index="1">sha1</numIndex>
                                    </numIndex>
                                    <numIndex index="1">
                                        <numIndex index="0">LLL:EXT:falsftp/Resources/Private/Language/locallang.xlf:flexform.experts.foreignKeyFingerprintMethod.md5</numIndex>
                                        <numIndex index="1">md5</numIndex>
                                    </numIndex>
                                </items>
                            </config>
                        </TCEforms>
                    </foreignKeyFingerprintMethod>
                    <folderCreateMask>
                        <TCEforms>
                            <label>LLL:EXT:falsftp/Resources/Private/Language/locallang.xlf:flexform.experts.folderCreateMask</label>
                            <config>
                                <type>input</type>
                                <size>33</size>
                                <eval>trim</eval>
                            </config>
                        </TCEforms>
                    </folderCreateMask>
                    <fileCreateMask>
                        <TCEforms>
                            <label>LLL:EXT:falsftp/Resources/Private/Language/locallang.xlf:flexform.experts.fileCreateMask</label>
                            <config>
                                <type>input</type>
                                <size>33</size>
                                <eval>trim</eval>
                            </config>
                        </TCEforms>
                    </fileCreateMask>
                </el>
            </ROOT>
        </experts>
    </sheets>
</T3DataStructure>
